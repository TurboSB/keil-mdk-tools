{
  "name": "keil-mdk-tools",
  "displayName": "Keil MDK Tools",
  "description": "load and build Keil uVision projects",
  "version": "1.0.4",
  "publisher": "TurboSB",
  "author": {
    "name": "TurboSB"
  },
  "keywords": [
    "keil",
    "mdk",
    "stm32"
  ],
  "homepage": "https://github.com/TurboSB/keil-mdk-tools",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "res/icon.png",
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TurboSB/keil-mdk-tools.git"
  },
  "bugs": {
    "url": "https://github.com/TurboSB/keil-mdk-tools/issues"
  },
  "contributes": {
    "icons": {
      "keil-mdk-build": {
        "description": "build icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e615"
        }
      },
      "keil-mdk-rebuild": {
        "description": "rebuild icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e616"
        }
      },
      "keil-mdk-download": {
        "description": "download icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e618"
        }
      },
      "keil-mdk-switch": {
        "description": "switch icon",
        "default": {
          "fontPath": "./res/iconfont.woff",
          "fontCharacter": "\\e617"
        }
      }
    },
    "configuration": [
      {
        "title": "%extension.name%",
        "properties": {
          "keil-mdk-tools.MDK.Uv4Path": {
            "type": "string",
            "scope": "machine",
            "markdownDescription": "%extension.settings.mdk%",
            "default": "null"
          },
          "keil-mdk-tools.C51.Uv4Path": {
            "type": "string",
            "scope": "machine",
            "markdownDescription": "%extension.settings.c51%",
            "default": "null"
          },
          "keil-mdk-tools.UserDefines": {
            "type": "string",
            "editPresentation": "multilineText",
            "scope": "window",
            "markdownDescription": "%extension.settings.userDefines%",
            "default": ""
          },
          "keil-mdk-tools.ActiveTarget": {
            "type": "string",
            "scope": "window",
            "markdownDescription": "%extension.settings.activeTarget%",
            "default": ""
          }
        }
      }
    ],
    "commands": [
      {
        "command": "keil-mdk-tools.project.build",
        "title": "%extension.commands.build.title%"
      },
      {
        "command": "keil-mdk-tools.project.rebuild",
        "title": "%extension.commands.rebuild.title%"
      },
      {
        "command": "keil-mdk-tools.project.download",
        "title": "%extension.commands.download.title%"
      },
      {
        "command": "keil-mdk-tools.project.switch",
        "title": "%extension.commands.switch.title%"
      }
    ],
    "problemMatchers": [
      {
        "name": "armcc",
        "fileLocation": "autoDetect",
        "pattern": [
          {
            "regexp": "^([^\\(]+)\\(([\\d]+)\\):\\s+(error|warning):\\s+#([\\d\\w-]+):\\s+(.+)$",
            "file": 1,
            "location": 2,
            "severity": 3,
            "code": 4,
            "message": 5
          }
        ]
      },
      {
        "name": "gcc",
        "fileLocation": "autoDetect",
        "pattern": [
          {
            "regexp": "^(.+):(\\d+):(\\d+):\\s+(\\w+):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.67.0",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vscode/test-electron": "^2.3.5",
    "@vscode/vsce": "2.21.1",
    "eslint": "^8.51.0",
    "fast-xml-parser": "^4.3.2",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {},
  "extensionDependencies": [
    "ms-vscode.cpptools"
  ]
}